{
  "name": "@arc/ai",
  "version": "0.2.0",
  "private": true,
  "type": "module",
  "description": "AI provider implementations for Arc chat application",
  "main": "./dist/ai.js",
  "types": "./dist/ai.d.ts",
  "exports": {
    "./ai.js": {
      "types": "./src/ai.ts",
      "import": "./src/ai.ts",
      "default": "./src/ai.ts"
    },
    "./provider.js": {
      "types": "./src/provider.type.ts",
      "import": "./src/provider.type.ts",
      "default": "./src/provider.type.ts"
    },
    "./provider.type.js": {
      "types": "./src/provider.type.ts",
      "import": "./src/provider.type.ts",
      "default": "./src/provider.type.ts"
    },
    "./errors.js": {
      "types": "./src/errors.ts",
      "import": "./src/errors.ts",
      "default": "./src/errors.ts"
    },
    "./providers/*.js": {
      "types": "./src/providers/*.ts",
      "import": "./src/providers/*.ts",
      "default": "./src/providers/*.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "check-types": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "dependencies": {
    "@arc/platform": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
